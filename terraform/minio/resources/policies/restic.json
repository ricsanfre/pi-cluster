{
  "name": "restic",
  "description": "Restic backup access policy",
  "statements": [
    {
      "effect": "Allow",
      "actions": [
        "s3:DeleteObject",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:PutObject"
      ],
      "resources": [
        "arn:aws:s3:::restic",
        "arn:aws:s3:::restic/*"
      ]
    }
  ]
}
